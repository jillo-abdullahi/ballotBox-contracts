# Environment Variables for Deployment
# Copy this to .env and fill in your actual values

# Sepolia RPC URL (get from Infura, Alchemy, or other provider)
RPC_URL_SEPOLIA=https://sepolia.infura.io/v3/YOUR_PROJECT_ID

# Private key of the account you want to deploy from
# NEVER commit your real private key to git!
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here
